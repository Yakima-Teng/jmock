{
  "name": "jmock",
  "version": "1.0.2",
  "description": "A simple command-line http server for mocking data, proxying requests and serving static files",
  "main": "./lib/jmock",
  "repository": {
    "type": "git",
    "url": "git://github.com/Yakima-Teng/jmock.git"
  },
  "keywords": [
    "cli",
    "command",
    "static",
    "http",
    "https",
    "http-server",
    "https-server",
    "server",
    "mock",
    "proxy",
    "jmock"
  ],
  "scripts": {
    "ready": "npm install",
    "start": "node ./bin/jmock -p 8081 --config",
    "test": "tap --reporter=spec test/*.test.js",
    "test-watch": "tap --reporter=spec --watch test/*.test.js",
    "release": "bash bash/release.sh"
  },
  "files": [
    "lib",
    "bin",
    "doc",
    "jmock.config.js"
  ],
  "man": "./doc/jmock.1",
  "engines": {
    "node": ">=12.16"
  },
  "contributors": [
    {
      "name": "Yakima Teng",
      "email": "yakima.public@gmail.com"
    }
  ],
  "dependencies": {
    "basic-auth": "^2.0.1",
    "chalk": "^4.1.2",
    "co-body": "^6.1.0",
    "corser": "^2.0.1",
    "he": "^1.2.0",
    "html-encoding-sniffer": "^3.0.0",
    "http-proxy": "^1.18.1",
    "mime": "^1.6.0",
    "minimist": "^1.2.6",
    "mockjs": "^1.1.0",
    "opener": "^1.5.1",
    "portfinder": "^1.0.28",
    "secure-compare": "3.0.1",
    "union": "~0.5.0",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "eol": "^0.9.1",
    "eslint": "^4.19.1",
    "eslint-config-populist": "^4.2.0",
    "express": "^4.17.1",
    "request": "^2.88.2",
    "tap": "^14.11.0"
  },
  "bugs": {
    "url": "https://github.com/Yakima-Teng/jmock/issues"
  },
  "license": "MIT",
  "preferGlobal": true,
  "bin": {
    "jmock": "./bin/jmock"
  }
}
